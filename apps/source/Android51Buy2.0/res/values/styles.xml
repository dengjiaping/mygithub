<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="global_radio_sort">
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">0.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@drawable/none</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="global_listview">
        <item name="android:fadingEdge">none</item>
        <item name="android:dividerHeight">0.6dip</item>
        <item name="android:divider">#ffe9e9e9</item>
        <item name="android:cacheColorHint">#00000000</item>
    </style>
    
    <style name="global_listview_no_divider">
        <item name="android:fadingEdge">none</item>
        <item name="android:divider">@null</item>
        <item name="android:cacheColorHint">#00000000</item>
    </style>

    <style name="global_scrollview">
        <item name="android:fadingEdge">none</item>
        <item name="android:background">@color/global_bg</item>
    </style>

    <color name="global_divider">#ffe9e9e9</color>

    <style name="global_label">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#ff444444</item>
    </style>

    <style name="global_arrow_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/i_global_arrow_left</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>

    <style name="global_arrow_right">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/i_global_arrow_right</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="global_radiobutton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="global_header">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/i_header_bg</item>
    </style>

    <style name="global_round_top">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/x_global_round_top</item>
    </style>

    <style name="global_round_single">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/x_global_round_single</item>
    </style>

    <style name="global_line1">
        <item name="android:background">#eee9e9e9</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.6dip</item>
    </style>

    <style name="main_tab">
        <item name="android:gravity">center_horizontal|bottom</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:paddingLeft">0dip</item>
        <item name="android:paddingRight">0dip</item>
        <item name="android:paddingTop">0dip</item>
        <item name="android:paddingBottom">3dip</item>
        <item name="android:button">@null</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>

    <!-- style name="global_tab_gray">
        <item name="android:background">@drawable/x_global_tab_gray</item>
        <item name="android:button">@null</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style -->
 
    <style name="starRatingBar" parent="@android:style/Widget.RatingBar">  
        <item name="android:progressDrawable">@drawable/star_ratingbar</item>
        <item name="android:minHeight">48dip</item>  
        <item name="android:maxHeight">48dip</item>  
    </style>
    
    <style name="DialogText">
        <item name="android:textColor">#FF000000</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="DialogText.Title">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    
    <style name="themeDaytime" parent="@android:style/Theme">
        <item name="android:colorForeground">#00000000</item>
    </style>
    
    <style name="themeNight" parent="@android:style/Theme">
        <item name="android:colorForeground">@color/off_mode_foreground</item>
    </style>
    
    <!-- 51buy 2.0 text -->
    <style name="text_caption">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/textsize_small</item>
        <item name="android:textColor">#ff444444</item>
    </style>
    
    <style name="text_message">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
    </style>
    
    <style name="text_subtitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#ffcccccc</item>
    </style>
    
    <style name="home_price_caption">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/home_price_size</item>
    </style>
    <style name="text_info">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#ffcccccc</item>
    </style>
    
    <style name="timing_num">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/global_white</item>
        <item name="android:background">@drawable/timing_bk</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">2dp</item>
        <item name="android:text">0</item>
    </style>
    
    <style name="denom_option">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/margin_size_90xp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginLeft">6dp</item>
        <item name="android:layout_marginRight">6dp</item>
        <item name="android:background">@drawable/denom_off</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/global_label</item>
        <item name="android:textSize">16sp</item>
    </style>
    
    <!-- 51buy 2.0 image -->
    <style name="image_icon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">4dp</item>
    </style>
    
    <style name="timing_line">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:src">@drawable/timing_line</item>
    </style>
    
    <style name="search_listview_hotkey">
        <item name="android:fadingEdge">none</item>
        <item name="android:dividerHeight">1dp</item>
        <item name="android:divider">#ffe5e5e5</item>
        <item name="android:cacheColorHint">#00000000</item>
    </style>
    
    <style name="list_activity_listview">
        <item name="android:fadingEdge">none</item>
        <item name="android:dividerHeight">2px</item>
        <item name="android:divider">#fff0f0f0</item>
        <item name="android:cacheColorHint">#00000000</item>
    </style>
    
    <style name="button_confirm_bg_white">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/item_detail_btn</item>
        <item name="android:textColor">@color/global_text_color</item>
        <item name="android:textSize">@dimen/front_size_36xp</item>
    </style>
    
    <style name="button_confirm_bg_blue">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/x_button_submit_blue</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/front_size_36xp</item>
    </style>
    
    <style name="button_confirm_bg_orange">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/confirm_button</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/front_size_36xp</item>
    </style>
    
    <style name="filter_checkbox">
        <item name="android:button">@drawable/x_checkbox_filter</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    <style name="filter_line">
        <item name="android:background">#ffff0000</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">2px</item>
    </style>
    
    <style name="global_scrollview_gray">
        <item name="android:fadingEdge">none</item>
        <item name="android:background">@color/background_color</item>
    </style>
    
    <style name="global_line2">
        <item name="android:background">@color/global_line</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">2px</item>
    </style>

    <style name="confirm_button">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:background">@drawable/confirm_button</item>
    </style>
    
     <style name="text_settings_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">@color/global_text_color</item>
    </style>
    
     <style name="text_settings_info">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/global_text_info_color</item>
    </style>
    
    <style name="dropdown_translucent">
        <item name="android:windowBackground">@color/transparent</item>  
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowAnimationStyle">@style/push_in_out</item>
        <item name="android:backgroundDimAmount">0.4</item><!-- 灰度 -->
    </style>
    
    <style name="push_in_out">
        <item name="android:windowEnterAnimation">@anim/push_down_in</item>
        <item name="android:windowExitAnimation">@anim/push_up_out</item>
    </style>
    
    
    <style name="boom_in_out">
        <item name="android:windowEnterAnimation">@anim/center_boom_in</item>
        <item name="android:windowExitAnimation">@anim/center_boom_out</item>
    </style>
    
    <style name="leftIn_rightOut">
        <item name="android:activityOpenEnterAnimation">@anim/in_rightleft</item>
        <item name="android:activityOpenExitAnimation">@anim/out_rightleft</item>
        <item name="android:activityCloseEnterAnimation">@anim/in_leftright</item>
        <item name="android:activityCloseExitAnimation">@anim/out_leftright</item>
    </style>
    
    <style name="FilterAnimation" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowAnimationStyle">@style/leftIn_rightOut</item>
    </style>
    
    <style name="transparent_theme">  
    	<item name="android:windowBackground">@color/transparent</item>  
    	<item name="android:windowNoTitle">true</item>  
    	<item name="android:windowIsTranslucent">true</item>    
    	<item name="android:backgroundDimEnabled">true</item>
    	<item name="android:backgroundDimAmount">0.4</item><!-- 灰度 -->
    </style> 
    <style name="DialogAnimation">
        <item name="android:windowEnterAnimation">@anim/push_up_in</item>
        <item name="android:windowExitAnimation">@anim/push_down_out</item>
    </style>
    
     <style name="AddressDialogAnimation">
        <item name="android:windowEnterAnimation">@anim/fly_right_in</item>
        <item name="android:windowExitAnimation">@anim/fly_right_out</item>
    </style>
    
</resources>