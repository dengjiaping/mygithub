<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@color/background_color"
        android:baselineAligned="false"
        android:paddingBottom="1dip" >

        <LinearLayout
            android:id="@+id/time_buy_container_1"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginRight="1dip"
            android:layout_weight="1"
            android:background="@drawable/global_white_shadow_click_state"
            android:orientation="horizontal"
            android:padding="@dimen/margin_size_10xp" >

            <RelativeLayout
                android:id="@+id/time_buy_img_ll"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <ImageView
                    android:id="@+id/time_buy_image_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/app_name"
                    android:paddingBottom="5dp"
                    android:paddingTop="@dimen/margin_size_10xp"
                    android:scaleType="fitCenter" >
                </ImageView>

                <!-- ImageView
                    android:id="@+id/time_buy_image_sale_out_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/app_name"
                    android:scaleType="fitCenter"
                    android:src="@drawable/timebuy_sale_out" >
                </ImageView -->

                <!-- ImageView
                    android:id="@+id/time_buy_image_not_sale_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:contentDescription="@string/app_name"
                    android:scaleType="fitCenter"
                    android:src="@drawable/timebuy_not_sale" / -->
            </RelativeLayout>

            <RelativeLayout                 
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/margin_size_10xp"
                android:paddingRight="@dimen/margin_size_10xp"
                android:paddingTop="@dimen/margin_size_10xp"
                >
                <TextView
                    android:id="@+id/list_prop_name1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom"
                    android:singleLine="true"
                    android:textColor="#ff313131"
                    android:layout_marginBottom="@dimen/margin_size_20xp"
                    android:textSize="@dimen/font_size_30xp" />
                
                <TextView
                    android:id="@+id/list_price_qq1_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom"
                    android:singleLine="true"
                    android:textColor="@color/global_text_info_color"
                    android:text="@string/price_wechat"
                    android:layout_below="@id/list_prop_name1"
                    android:textSize="@dimen/font_size_26xp" />
                
                <TextView
                    android:id="@+id/list_price_qq1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom"
                    android:layout_below="@id/list_prop_name1"
                    android:layout_toRightOf="@id/list_price_qq1_title"
                    android:singleLine="true"
                    android:textColor="@color/red"
                    android:textSize="@dimen/font_size_26xp" />
                
                <TextView
                    android:id="@+id/list_price_wechat1_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom"
                    android:singleLine="true"
                    android:layout_below="@id/list_price_qq1_title"
                    android:textColor="@color/global_text_info_color"
                    android:text="@string/price_qq"
                    android:textSize="@dimen/font_size_26xp" />
                <TextView
                    android:layout_below="@id/list_price_qq1_title"
                    android:layout_toRightOf="@id/list_price_wechat1_title"
                    android:id="@+id/list_price_wechat1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom"
                    android:singleLine="true"
                    android:textColor="@color/red"
                    android:textSize="@dimen/font_size_26xp" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/time_buy_container_2"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_marginRight="1dip"
            android:layout_weight="1"
            android:background="@drawable/global_white_shadow_click_state"
            android:orientation="horizontal"
            android:padding="@dimen/margin_size_10xp" >

            <RelativeLayout
                android:id="@+id/time_buy_img_ll_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <ImageView
                    android:id="@+id/time_buy_image_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/app_name"
                    android:paddingBottom="5dp"
                    android:paddingTop="@dimen/margin_size_10xp"
                    android:scaleType="fitCenter" >
                </ImageView>

                <!-- ImageView
                    android:id="@+id/time_buy_image_sale_out_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/app_name"
                    android:scaleType="fitCenter"
                    android:src="@drawable/timebuy_sale_out" >
                </ImageView -->

                <!-- ImageView
                    android:id="@+id/time_buy_image_not_sale_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:contentDescription="@string/app_name"
                    android:scaleType="fitCenter"
                    android:src="@drawable/timebuy_not_sale" / -->
            </RelativeLayout>
            
            <RelativeLayout                 
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/margin_size_10xp"
                android:paddingRight="@dimen/margin_size_10xp"
                android:paddingTop="@dimen/margin_size_10xp"
                >
                <TextView
                    android:id="@+id/list_prop_name2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom"
                    android:singleLine="true"
                    android:layout_marginBottom="@dimen/margin_size_20xp"
                    android:textColor="#ff313131"
                    android:textSize="@dimen/font_size_30xp" />
                
                <TextView
                    android:id="@+id/list_price_qq2_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom"
                    android:singleLine="true"
                    android:textColor="@color/global_text_info_color"
                    android:text="@string/price_wechat"
                    android:layout_below="@id/list_prop_name2"
                    android:textSize="@dimen/font_size_26xp" />
                
                <TextView
                    android:id="@+id/list_price_qq2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom"
                    android:layout_below="@id/list_prop_name2"
                    android:layout_toRightOf="@id/list_price_qq2_title"
                    android:singleLine="true"
                    android:textColor="@color/red"
                    android:textSize="@dimen/font_size_26xp" />
                
                <TextView
                    android:id="@+id/list_price_wechat2_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom"
                    android:singleLine="true"
                    android:layout_below="@id/list_price_qq2_title"
                    android:textColor="@color/global_text_info_color"
                    android:text="@string/price_qq"
                    android:textSize="@dimen/font_size_26xp" />
                <TextView
                    android:layout_below="@id/list_price_qq2_title"
                    android:layout_toRightOf="@id/list_price_wechat2_title"
                    android:id="@+id/list_price_wechat2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom"
                    android:singleLine="true"
                    android:textColor="@color/red"
                    android:textSize="@dimen/font_size_26xp" />
            </RelativeLayout>
           
        </LinearLayout>
    </LinearLayout>

    <Button
        android:layout_width="0px"
        android:layout_height="0px" />

</RelativeLayout>