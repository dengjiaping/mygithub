<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:djcity="http://schemas.android.com/apk/res/com.tencent.djcity"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >

    <com.tencent.djcity.lib.ui.NavigationBar
    	android:id="@+id/discover_navbar"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        djcity:text="@string/discover"/>
        
    
    <ListView
    	android:layout_below="@id/discover_navbar"
       	android:id="@+id/discover_listview"
       	style="@style/global_listview"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" />
        
 </RelativeLayout>