<resources>

    <style name="unipay_tenpay_translucent">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="unipay_tenpay_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="unipay_tenpay_wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="unipay_tenpay_fill">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="unipay_tenpay_linear">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="unipay_tenpay_linear_hori">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="unipay_tenpay_linear.margin">
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="unipay_tenpay_fill.center">
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">#90000000</item>
    </style>

    <style name="unipay_tenpay_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/unipay_tenpay_btn_color</item>
    </style>

    <style name="unipay_tenpay_text.middle">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="unipay_tenpay_text.small">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="unipay_tenpay_text.middlethin">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/unipay_tenpay_thin_color</item>
